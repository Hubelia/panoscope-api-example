{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "@nuxtjs/eslint-config-typescript",
    "plugin:vue/vue3-recommended"
  ],
  "ignorePatterns": ["dist"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "max-len": ["error", { "code": 120 }],
    "no-unused-vars": "warn",
    "semi": ["error", "never"],
    "vue/multi-word-component-names": "off",
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "vue/no-multiple-template-root": "off",
    "import/order": "off"
  }
}
